# Auto-Training Microservice env (example)
NODE_ENV=development
PORT=3001

# CORS
ALLOWED_ORIGINS=http://localhost:3000,https://www.mouna-ai.com

# Shared internal key with main app (must match main app INTERNAL_SERVICE_KEY)
INTERNAL_SERVICE_KEY=change_this_internal_service_key
# Optionally accept a main app API key as well
# MAIN_APP_API_KEY=

# OpenAI (optional but recommended for better analysis/FAQs)
OPENAI_API_KEY=your_openai_api_key_here

# Database
MONGODB_URI=mongodb://localhost:27017/ai-chatbot

